## The function is currently defined as
function (x, popSize, weights) 
{
    pairs = matrix(NA, ncol = popSize/2, nrow = 2)
    set.seed(0)
    for (i in 1:popSize/2) {
        pairs[, i] = sample(1:popSize, 2, prob = weights)
    }
    pairs
    xSamp = x[as.vector(pairs), ]
    return(xSamp)
  }
}
